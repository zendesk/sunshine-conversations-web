  "scripts": {
    "test": "karma start karma.conf.js --no-single-run --reporters spec --browsers PhantomJS,Chrome",
    "test:phantom": "karma start karma.conf.js --no-single-run --reporters spec --browsers PhantomJS",
    "karma": "karma start karma.conf.js --reporters spec --browsers PhantomJS",
    "test:ci": "karma start karma.conf.js --single-run --reporters junit --browsers PhantomJS",
    "build": "rm -rf dist && npm run build:host && npm run build:frame",
    "build:host": "webpack --config webpack-host.config.js --progress --profile --colors",
    "build:frame": "webpack --config webpack-frame.config.js --progress --profile --colors",
    "build:npm": "rm -rf lib && webpack --config webpack-host-npm.config.js --progress --profile --colors",
    "start": "node dev-server/server-production",
    "lint": "eslint . --ext=js --ext=jsx",
    "dev": "nodemon --watch dev-server/ --watch src/index.html --watch src/embedded.html --watch config/config.json dev-server/server-development"
  },
  "main": "lib/index.js",
  "homepage": "https://smooch.io",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=4.2.6",
    "npm": ">=3.6.0"
  },
