{
  "name": "smooch",
  "analyze": false,
  "version": "2.0.8",
  "keywords": [
    "smooch",
    "javascript",
    "sdk"
  ],
  "author": {
    "name": "Andrew Lavers",
    "org": "Smooch"
  },
  "scripts": {
    "test": "karma start karma.conf.js --no-single-run --reporters progress --browsers PhantomJS,Chrome",
    "test:phantom": "karma start karma.conf.js --no-single-run --reporters progress --browsers PhantomJS",
    "karma": "karma start karma.conf.js --reporters spec --browsers PhantomJS",
    "test:ci": "karma start karma.conf.js --single-run --reporters junit --browsers PhantomJS",
    "dev-server": "webpack-dev-server --config webpack-dev-server.config.js --colors --port 2992 --inline  --no-info",
    "hot-dev-server": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --colors --port 2992 --inline --no-info",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors",
    "start-dev": "node lib/server-development",
    "start": "node lib/server-production",
    "lint": "eslint . --ext=js --ext=jsx"
  },
  "main": "dist/smooch.js",
  "homepage": "http://smooch.io",
  "license": "http://smooch.io/terms.html",
